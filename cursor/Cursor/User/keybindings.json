    },
    {
        "key": "ctrl+g",
        "command": "editor.action.inlineDiffs.rejectAll",
        "when": "editorTextFocus && @inlineDiffs.rejectAllEdits.isActiveEditorWithDiffs"
    },
    {
        "key": "shift+cmd+backspace",
        "command": "-editor.action.inlineDiffs.rejectAll",
        "when": "editorTextFocus && @inlineDiffs.rejectAllEdits.isActiveEditorWithDiffs"
